#include<stdio.h>
typedef int array[3][3];
int main()
{
    array arr={1,2,3,4,5,6,7,8,9};
    array *ptr;
    ptr=&arr;
    printf("array elements:\n");
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            printf("%d",(*ptr)[i][j]);
        }
        printf("\n");
    }
}

output:

array elements:
123
456
789
