#include<stdio.h>
int main()
{
    int h=100;
    int c;
  
    c=h*(1-(1/2)^5)/(1-(1/2));/* by using geometric expression s=a(r^n - 1)/(r^n - 1)  if r>1
                                                                 a(1 - r^n)/(1 - r^n)  if r<1
                                                                 an         if r=1
                                                                 */
    printf("%d",c);
}
