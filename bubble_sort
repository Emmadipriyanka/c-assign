#include<stdio.h>
int main()
{
    int arr[]={2,5,1,7,9,11,6};
    for(int i=0;i<7-1;i++)
    {
        for(int j=0;j<7-i-1;j++)
        {
            if(arr[j]>arr[j+1])
            {
                int temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
    printf("bubble sort:");
    for(int i=0;i<7;i++)
        printf("%d\t",arr[i]);
}

output:

bubble sort:1   2       5       6       7       9       11
