#include<stdio.h>
int main()
{
    int arr[]={2,5,1,7,9,11,6};
    for(int i=0;i<7-1;i++)
    {
        for(int j=i+1;j<7;j++)
        {
            if(arr[i]>arr[j])
            {
                int temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            }
        }
    }
    printf("bubble sort:");
    for(int i=0;i<7;i++)
        printf("%d\t",arr[i]);
}

output:

bubble sort:1   2       5       6       7       9       11
