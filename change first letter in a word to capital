#include<stdio.h>
int main(){
    char s[100];
    scanf("%[^\n]%*c",s); 
    if(s[0]>= 'a' && s[0]<= 'z')
        s[0] = s[0] - 32;
    for(int i=1;s[i]!='\0';i++){
        if(s[i] == ' '){
            if(s[i+1]>= 'a' && s[i+1]<= 'z'){
                s[i+1] = s[i+1] - 32;
            }
        }
    }
    for(int i=0;s[i]!='\0';i++)
        printf("%c",s[i]);
    
}
