#include<stdio.h>
#include<string.h>
int main(){
    char s[100];
    scanf("%[^\n]%*c",s); 
    if(s[0] == 'a' || s[0] == 'e' || s[0] == 'i' || s[0] == 'o' || s[0] == 'u')
        s[0] = s[0] - 32;
    for(int i=1;s[i]!='\0';i++){
        if(s[i] == ' '){
            if(s[i+1] == 'a' || s[i+1] == 'e' || s[i+1] == 'i' || s[i+1] == 'o' || s[i+1] == 'u'){
                s[i] = s[i] - 32;
            }
        }
    }
    for(int i=0;s[i]!='\0';i++)
        printf("%c",s[i]);
    
}
