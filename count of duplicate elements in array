#include<stdio.h>
int main(){
    int arr[10]= {1,1,2,3,4,2,3,4,1,10}, count = 1;
    for(int i=0;i<10;i++){
        for(int j=i+1;j<10;j++){
            if(arr[i] == arr[j]){
                count++;
                arr[j] = -1;
            }
        }
        if(arr[i] != -1)
            printf("count of %d in given array is : %d\n",arr[i],count);
        count = 1;
    }
}
