#include<stdio.h>
#include<string.h>
int main()
{
    char a[100],b[100];
    scanf("%s",a);
    int c=0;
    int e[10];
    scanf("%s",b);
    int d=strlen(b);
    for(int i=0,j=0,k=0;b[k]!='\0';i++)
    {
        if(a[i]==b[k]){
            c++;
            e[j]=i;
            j++;
            k++;
        } 
    }
    int f=-1;
    if(d==c)
        printf("%d",e[0]);
    else
        printf("%d",f);
}
