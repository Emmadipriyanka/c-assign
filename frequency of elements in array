#include<stdio.h>
int main()
{
    int arr[10]={1,1,1,2,3,2,3,4,5};
    int counter[100];
    int i,j;
    int count;
    for(i=0;i<10;i++)
    {
        count=1;
        for(j=i+1;j<10;j++)
        {
            if(arr[i]==arr[j])
            {
                count=count+1;
                counter[j]=-1;
            }
        }
        if(counter[i]!=-1)
        {
            counter[i]=count;
        }
        
    }
    for(int i=0;i<10;i++)
    {
        if(counter[i]!=-1)
        {
            printf("frequency of %d is %d\n",arr[i],counter[i]);
        }
    }
       
    
}

output: 

frequency of 1 is 3
frequency of 2 is 2
frequency of 3 is 2
frequency of 4 is 1
frequency of 5 is 1
frequency of 0 is 1
