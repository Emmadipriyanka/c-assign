
#include <stdio.h>
#include<string.h>


int main()
{
    char str1[30];
    int i,count=0;
    char ch;
    
    printf("Enter string1:\n");
    scanf("%[^\n]s",str1);
    printf("Enter the character:\n");
    scanf(" %c",&ch);
    for(i=0;str1[i]!='\0';i++)
    {
       if(str1[i]==ch)
       {
           count++;
       }
    }

        printf("Character %c is found these %d times ",ch,count);
    
    return 0;
}
