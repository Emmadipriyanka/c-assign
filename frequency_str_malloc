#include <stdio.h>
#include<stdlib.h>
int main()
{
    int n;
    printf("enter the size of string:");
    scanf("%d",&n);
    int b[n];
    char *ptr=(char *)malloc(n*sizeof(char));
    scanf("%s",ptr);
    for(int i=0;ptr[i]!='\0';i++)
    {
        int d=1;
         for(int j=i+1;ptr[j]!='\0';j++)
         {
             if(ptr[i]==ptr[j])
             {
                 d++;
                 b[j]=-100;
             }
         }
         if(b[i]!=-100)
         b[i]=d;
    }
     for(int i=0;ptr[i]!='\0';i++)
     {
          if(b[i]!=-100)
          printf("%c %d\n",ptr[i],b[i]);
     }
    return 0;
}
