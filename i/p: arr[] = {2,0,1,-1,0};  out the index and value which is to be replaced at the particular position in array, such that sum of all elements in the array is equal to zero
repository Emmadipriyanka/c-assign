#include<stdio.h>
int main(){
    int n;
    printf("enter size of an array:");
    scanf("%d",&n);
    int arr[n];
    printf("enter array elements :");
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
    if((n%2) != 0){
        arr[n-1] = 0;
        for(int i=0;i<(n-1);i=i+2){
            if((arr[i] + arr[i+1]) != 0)
                arr[i+1] = (-arr[i]);
        }
    }
    else{
        for(int i=0;i<n;i=i+2){
            if((arr[i] + arr[i+1]) != 0)
                arr[i+1] = (-arr[i]);
        }
    }
    for(int i=0;i<n;i++)
        printf("%d ",arr[i]);
}
