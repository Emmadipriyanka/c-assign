#include<stdio.h>
int main()
{
    int len;
    scanf("%d",&len);
    int arr[len];
    for(int i=0;i<len;i++)
    {
        scanf("%d",&arr[i]);
    }
    int sum=0;
    for(int i=0;i<len;i++)
    {
        sum=sum+arr[i];
    }
    int r=((len+1)*(len+2))/2;
    int ele=r-sum;
    printf("element=%d",ele);
}
