#include<stdio.h>
int main(){
    int arr[10];
    int count =0;
    printf("enter array elements:");
    for(int i=0;i<10;i++)
        scanf("%d",&arr[i]);
    for(int i=0;i<10;i++)
        if(arr[i]!=0)
            arr[count++] = arr[i];
     while (count < 10) 
            arr[count++] = 0;
    for(int i=0;i<10;i++)
        printf("%d ",arr[i]);
}
