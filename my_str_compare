
#include <stdio.h>
#include<string.h>

void mystrcmp(char str[],char str1[])
{
    int count=0;
    int l1=strlen(str);
    int l2=strlen(str1);
    
    if(l1==l2)
    {
        for(int i=0;i<l1;i++)
        {
            if(str[i]==str1[i])
            count++;
        }
        if(count==l1)
        printf("strings are equal");
        else
        printf("strings are not equal");
    }
    
   else if(l1>l2)
   printf("String1 is greater than string2 and its return type is 1");
   
   else
   printf("String2 is greater than string1 and its return type is -1");
}

int main()
{
    char str[30];
    char str1[30];
    void (*fun)(char[],char[]);
    
    printf("Enter string1:\n");
    scanf("%[^\n]s",str);
    printf("Enter string2:\n");
    scanf(" %[^\n]s",str1);
    
    fun=mystrcmp;
    fun(str,str1);
   
   
   
    return 0;
}
