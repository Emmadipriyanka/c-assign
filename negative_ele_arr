#include<stdio.h>
#define len 10
int fun(int arr[])
{
    int count=0;
    for(int i=0;i<len;i++)
    {
        if(arr[i]<0)
            count=count+1;
    }
    return count;
}
int main()
{
    int arr[len];
    printf("enter array elements:");
    for(int i=0;i<len;i++)
    {
        scanf("%d",&arr[i]);
    }
    int res=fun(arr);
    printf("no.of negative elements in an array is: %d\n",res);
}

output:

enter array elements:5 2 -9 10 0 6 7 -1 -4 -6
no.of negative elements in an array is: 4
