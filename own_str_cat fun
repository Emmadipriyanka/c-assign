#include<stdio.h>
void str_cat(char s1[],char s2[])
{
    char s[20];
    int i=0,j=0,k=0;
    for(i=0;s1[i]!='\0';i++)
        s[i]=s1[i];
    j=i;
    s[j]=32;
    for(k=0;s2[k]!='\0';k++)
    {
        s[j]=s2[k];
        j++;
    }
    s[j]='\0';
    printf("%s",s);
}
int main()
{
    char s1[10]="priya";
    char s2[10]="hani";
    // printf("enter first string:");
    // fgets(s1,10,stdin);
    // printf("enter second string:");
    // fgets(s2,10,stdin);
    void(*fun)(char[],char[]);
    fun=str_cat;
    fun(s1,s2);
}
