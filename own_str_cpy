#include<stdio.h>

void str_cpy(char s1[])
{
    char s2[20];
    int i=0;
    for(i=0;s1[i]!='\0';i++)
        s2[i]=s1[i];
    s2[i]='\0';
    printf("copied string is : %s",s2);
}
int main()
{
    char s1[20];
    printf("enter string:");
    fgets(s1,20,stdin);
    void (*fun) (char []);
    fun=str_cpy;
    fun(s1);
}
