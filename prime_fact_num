#include<stdio.h>
int main()
{
    int n,i,j,prf;
    scanf("%d",&n);
    printf("prim factors of a given number %d is:",n);
    for(i=2;i<=n;i++)
    {
        if(n%i==0)
        {
            prf=1;
            for(j=2;j<=i/2;j++)
            {
                if(i%j==0)
                {
                    prf=0;
                    break;
                }
            }
            if(prf==1)
                printf("%d ",i);
        }
    }
}
