#include<stdio.h>
void priya_strcat(char [],char []);
#define len 100
int main()
{
    char s1[len],s2[len];
    printf("enter string s1:");
    scanf("%s",s1);
    printf("enter string s2:");
    scanf("%s",s2);
    priya_strcat(s1,s2);
}
void priya_strcat(char p[],char r[])
{
    int i=0,j=0,k=0;
    char s3[len];
    while(p[i]!='\0')
    {
        s3[k]=p[i];
        i++;
        k++;
    }
    i=0;
    while(r[j]!='\0')
    {
        s3[k]=r[j];
        j++;
        k++;
    }
    s3[k]='\0';
    printf("concatenation string is %s",s3);
}

output:

enter string s1:priya
enter string s2:hani
concatenation string is priyahani
