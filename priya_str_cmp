#include<stdio.h>
#define len 100
int priya_strcmp(char [],char []);
int main()
{
    char s1[len],s2[len];
    int cmp;
    printf("enter a string:");
    scanf("%s",s1);
    printf("enter a string:");
    scanf("%s",s2);
    cmp=priya_strcmp(s1,s2);
    if(cmp==0)
    {
        printf("given strings are same");
    }
    else
    {
        printf("given strings are not same");
    }
}
int priya_strcmp(char p[],char r[])
{
    int i=0,c=0;
    while(p[i]!='\0' && r[i]!='\0')
    {
        if(p[i]!=r[i])
        {
            c=1;
            break;
        }
        i++;
    }
    if(c==0)
        return 0;
    else
        return 1;
}

output:

***case1:
enter a string:priya
enter a string:priya
given strings are same

*****case 2:
enter a string:hani
enter a string:pinky
given strings are not same
