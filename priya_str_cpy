#include<stdio.h>
#define len 100
void priya_strcpy(char []);
int main()
{
    char s1[len];
    printf("enter a string:");
    scanf("%[^\n]s",s1);
    priya_strcpy(s1);
}
void priya_strcpy(char p[])
{
    char s2[len];
    for(int i=0;i<len;i++)
    {
        if(p[i]!='\0')
        {
            s2[i]=p[i];
        }
    }
    printf("copied string: %s",s2);
}

output:

enter a string:priya hani
copied string: priya hani
