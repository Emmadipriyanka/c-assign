#include<stdio.h>
#include<string.h>
int main()
{
    char str[20];
    fgets(str,20,stdin);
    int len=strlen(str);
    for(int i=0;i<len/2;i++)
    {
        char temp=str[i];
        str[i]=str[len-i-1];
        str[len-i-1]=temp;
    }
    printf("string after reverse is:");
    for(int i=0;i<len;i++)
        printf("%c",str[i]);
}
