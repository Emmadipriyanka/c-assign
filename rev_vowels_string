#include<stdio.h>
int main()
{
    char s[]="raghu";
    int arr[10],c=0;
    for(int i=0,j=0;s[i]!='\0';i++)
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u'){
            arr[j]=i;
            j++;
            c++;
        }
        
    }
    int j=0,k=0;
    for(int i=0;i<c/2;i++){
        j=arr[i];
        k=arr[c-i-1];
        char temp=s[j];
        s[j]=s[k];
        s[k]=temp;
    }
    printf("%s",s);
}
