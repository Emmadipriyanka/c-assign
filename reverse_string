#include <stdio.h>
#include<string.h>
int main()
{
    char str1[]={"hanish"};
    int len=strlen(str1);
    char str2[len];
    int j=0,i=0;
    for(i=len-i-1;i>=0;i--)
    {
        str2[j]=str1[i];
        j++;
    }
    printf("reverse string %s is",str2);
    return 0;
}


output:

reverse string hsinah is
