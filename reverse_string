
#include <stdio.h>
#include<string.h>


int main()
{
    char str1[30],str2[30];
    int l1;
    int i;
    
    printf("Enter string1:\n");
    scanf("%[^\n]s",str1);
    
    l1=strlen(str1);
    for(i=0;str1[i]!='\0';i++)
    {
       str2[i]=str1[l1-1];
       l1=l1-1;
    }
    
    printf("Reverse of string:%s",str2);
   
   
    return 0;
}
