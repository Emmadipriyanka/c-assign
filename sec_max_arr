#include<stdio.h>
#define len 10
int fun(int arr[])
{
    int max,sec_max,i;
    max=arr[0];
    sec_max=0;
    for(i=1;i<len;i++)
    {
        if(max<arr[i])
        {
            sec_max=max;
            max=arr[i];
        }
        else if(arr[i]>sec_max && arr[i]<max)
            sec_max=arr[i];
    }
    return sec_max;
}
int main()
{
    int arr[len];
    printf("enter array elements:");
    for(int i=0;i<len;i++)
    {
        scanf("%d",&arr[i]);
    }
    int res=fun(arr);
    printf("sec_max element is: %d\n",res);
}

output:

enter array elements:3 5 2 1 7 6 4 8 9 10
sec_max element is: 9
