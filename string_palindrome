#include<stdio.h>
#include<string.h>
int main()
{
    char str[10],s[10];
    int j=0;
    scanf("%s",str);
    int len=strlen(str);
    for(int i=0;i<len/2;i++)
    {
        if(str[i]!=str[len-i-1]){
            printf("not a palindrome");
            return 0;
        }
    }
    printf("palindrome");
}
