#include<stdio.h>
//void swap_withtemp(int,int); //function prototype of swapping two numbers using temp
//void swap_withouttemp(int,int); //function prototype of swapping two numbers without using temp
void swap_ptr(int*,int*); //function prototype of swapping two numbers using pointer
int main()
{
    int a=10,b=20;//declaration and inialization of a and b
    printf("before swapping a=%d  b=%d \n",a,b);
    //swap_withtemp(a,b);//calling a function by passing parameters as value
    //swap_withouttemp(a,b);//calling a function by passing parameters as value
    swap_ptr(&a,&b);//calling a function by passing parameters as pointers
    printf("after swapping a=%d b=%d",a,b);
}
/*void swap_withtemp(int a,int b)
{
    int temp;//declaration a temprary variable 
    temp=a;
    a=b;
    b=temp;
    printf("after swapping a=%d b=%d",a,b);
}
void swap_withouttemp(int a,int b)
{
    a=a+b;
    b=a-b;
    a=a-b;
    printf("after swapping a=%d b=%d",a,b);
}*/
void swap_ptr(int* a,int* b)
{
    int temp;//declaration a temporary variable
    temp=*a;
    *a=*b;
    *b=temp;
}

output:

before swapping a=10  b=20
after swapping a=20 b=10
