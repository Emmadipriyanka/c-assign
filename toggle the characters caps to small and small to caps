#include <stdio.h>

int main()
{
    char str[10]="PrIyaNka";
    int res=0;
    for(int i=0;str[i]!='\0';i++)
    {
        if(str[i]>='a' && str[i]<= 'z'){
            res=(int )str[i] - 32;
            str[i] = (char)res;
        } 
        else{
            res=(int )str[i] + 32;
            str[i] = (char)res;
        }  
    }
    printf("%s",str);
}
